{
  "name": "viluxe",
  "version": "1.0.0",
  "description": "Luxury real estate website for coastal villas",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w", "devserver": "live-server --browser=firefox",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css", "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrasputyera/viluxe.git"
  },
  "author": "Andras Putyera",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrasputyera/viluxe/issues"
  },
  "homepage": "https://github.com/andrasputyera/viluxe#readme",
  "devDependencies": {
    "node-sass": "^5.0.0"
  },
  "dependencies": {
    "auto-prefixer": "^0.4.2",
    "concat": "^1.0.3",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  }
}
